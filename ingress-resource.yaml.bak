apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:  name: ingress-resource
spec:
  rules:
  - host: myservicea.foo.org
    http:
      paths:
      - path: /hello
        pathType: Prefix
        backend:
          service:
            name: hello-app
            port:
              number: 8080
  ingressClassName: nginx
